<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PloggingApp.Pages.RankingPage"
             xmlns:leaderboard="clr-namespace:PloggingApp.Features.Leaderboard"
             xmlns:viewmodel="clr-namespace:PloggingApp.Pages.Leaderboard"
             x:DataType="viewmodel:RankingViewmodel"
             Title="LeaderboardPage">
    <VerticalStackLayout>
        <leaderboard:LeaderboardView BindingContext="{Binding LeaderboardViewModel}"></leaderboard:LeaderboardView>
    </VerticalStackLayout>
</ContentPage>