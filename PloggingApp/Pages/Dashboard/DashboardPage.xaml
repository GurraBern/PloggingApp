<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PloggingApp.Pages.DashboardPage"
             xmlns:views="clr-namespace:PloggingApp.MVVM.Views"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             Title="Dashboard">
    <VerticalStackLayout>
        <views:MapView BindingContext="{Binding MapViewModel}"></views:MapView>
        <Button Text="Done" Command="{Binding DisplayPopupCommand}"></Button>
    </VerticalStackLayout>
</ContentPage>