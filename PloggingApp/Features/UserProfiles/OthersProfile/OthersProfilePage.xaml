<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="PlogPal.Maui.Features.UserProfiles.OthersProfilePage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Others Profile"
    Shell.TabBarIsVisible="False">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Image
            Grid.Row="0"
            HorizontalOptions="Start"
            Source="leftarrow.png">
            <Image.GestureRecognizers>
                <TapGestureRecognizer Command="{Binding GoBackCommand}" Tapped="BackTap"/>
            </Image.GestureRecognizers>
        </Image>

        <!--<userProfileFeature:OthersSessionsView Grid.Row="1" BindingContext="{Binding OthersSessionsViewModel}"/>-->
    </Grid>
</ContentPage>