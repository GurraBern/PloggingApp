<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:PloggingApp.MVVM.Views"
             xmlns:viewmodel="clr-namespace:PloggingApp.Pages"
             x:DataType="viewmodel:MyProfilePageViewModel"
             x:Class="PloggingApp.Pages.MyProfilePage"
             Title="My Profile">

    <StackLayout>
        <views:MyProfileView BindingContext="{Binding MyProfileViewModel}"></views:MyProfileView>
    </StackLayout>
</ContentPage>




